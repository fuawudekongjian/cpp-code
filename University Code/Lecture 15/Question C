#include <iomanip>
#include <iostream>
using namespace std;
int main()
{
	int a[3][5], b[100][3] = { 0 }, c = 0;
	for (int i = 0; i < 3; i++)
	{
		for (int k = 0; k < 5; k++)
		{
			cin >> a[i][k];
			if (a[i][k] != 0)
			{
				b[c][0] = i + 1;
				b[c][1] = k + 1;
				b[c][2] = a[i][k];
				c++;
			}
		}
	}
	for (int i = 0; i < c; i++)
	{
		for (int k = 0; k < 3; k++)
			cout << fixed << setw(3) << b[i][k];
		cout << endl;
	}
	return 0;
}
