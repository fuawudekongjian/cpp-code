#include <iostream>
#include <string>
using namespace std;

int main()
{
	string a;
	getline(cin, a);
	int n = a.length();
	int ans_charge = 0, ans_word = 0;
	for (int i = 0; i < n; i++)
	{
		if (a[i] == '*')
		{
			ans_charge++;
			if (a[i + 1] != '*')
				ans_word++;
		}
	}
	cout << ans_charge << " " << ans_word;
	return 0;
}
