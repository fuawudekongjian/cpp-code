#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
	int n;
	cin >> n;

	int arr[1000000];
	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
	}
	sort(arr, arr + n);
	if (n % 2 == 1)
	{
		int i = n / 2;
		cout << arr[i];
	}
	else
	{
		int i = n / 2;
		cout << double((arr[i - 1] + arr[i]) )/ 2;
	}
	return 0;
}
