#include <iostream>
#include <string>
#include <cctype>
using namespace std;

int main()
{
	string a[3];
	for (int i = 0; i < 3; i++)
		getline(cin, a[i]);
	int ans_upper = 0, ans_lower = 0, ans_digit = 0, ans_space = 0, ans = 0;
	for (int i = 0; i < 3; i++)
	{
		int n = a[i].length();
		for (int k = 0; k < n; k++)
		{
			if (isupper(a[i][k]))ans_upper++;
			else if (islower(a[i][k]))ans_lower++;
			else if (isdigit(a[i][k]))ans_digit++;
			else if (isspace(a[i][k]))ans_space++;
			else ans++;
		}
	}
	cout << ans_upper << endl;
	cout << ans_lower << endl;
	cout << ans_digit << endl;
	cout << ans_space << endl;
	cout << ans;
	return 0;
}
