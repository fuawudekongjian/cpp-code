#include <iostream>
#include <string>
using namespace std;
int main()
{
	int n;
	cin >> n;
	string a;
	for (int i = 0; i < n; i++)
	{
		cin >> a;
		int x = a.length();
		int ans = 0;
		for (int k = 0; k < x; k++)
			ans += a[k];
		if (ans % 3 == 0)cout << "Yes" << endl;
		else cout << "No" << endl;
	}
}
