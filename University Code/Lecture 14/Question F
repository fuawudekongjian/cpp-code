#include <iostream>
#include <cstring>
#include <cmath>
using namespace std;
int main()
{
	int n, b;
	char a[1000];
	cin >> n;
	while (n--)
	{
		cin >> b >> a;
		int len = strlen(a), ans = 0;
		for (int i = 0; i < len; i++)
		{
			if (isdigit(a[i]))
				ans = ans * b + a[i] - '0';
			else
				ans = ans * b + a[i] - 'A' + 10;
		}
		cout << ans << endl;
	}
}
