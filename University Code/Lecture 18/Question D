#include <iostream>
#include <string>
using namespace std;

int main()
{
	string a;
	string ans ="apple";
	getline(cin, a);
	int n = a.length();
	bool b[100000] = { false };
	for (int i = 0; i < n; i++)
	{
		if (a[i] == '*' && !b[i])
		{
			a.replace(i, 5, ans);
			for (int k = i; k < i + 6; k++)
				b[i] = true;
		}
		else continue;
	}
	cout << a;
	return 0;
}
