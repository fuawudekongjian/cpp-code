#include <iostream>
#include <string>
using namespace std;

int main()
{
	string a;
	getline(cin, a);
	int ans = 0;
	int len = a.length();
	bool b[100000] = { false };
	for (int i = 0; i < len; i++)
	{
		int temp = 0;
		if (isdigit(a[i])&&b[i]==false)
		{
			temp = a[i] - '0';
			b[i] = true;
			for (int k = i + 1; k < len; k++)
			{
				if (isdigit(a[k]))
				{
					b[k] = true;
					temp = temp * 10 + a[k] - '0';
				}
				else
					break;
			}
		}
		ans += temp;
	}
	cout << ans;
	return 0;
}
