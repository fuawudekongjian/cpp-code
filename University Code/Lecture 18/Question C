#include <iostream>
#include <string>
using namespace std;

int main()
{
	string a;
	cin >> a;
	int n = a.length();
	int ans = 0;
	for (int i = 0; i < n; i++)
	{
		if (islower(a[i]))
		{
			ans += a[i] - 'a' + 1;
		}
		if (isupper(a[i]))
		{
			ans += a[i] - 'A' + 27;
		}
	}
	bool flag = true;
	for (int i = 2; i < ans; i++)
	{
		if (ans % i == 0)
		{
			flag = false;
			break;
		}
	}
	if (flag)cout << "yes";
	else cout << "no";
	return 0;
}
